<?xml version = "1.0" encoding = "UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
      http://www.springframework.org/schema/context
      http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<context:annotation-config />
	<!-- bean definitions go here -->

	<bean id="employee" class="com.example.demo.model.Employee">
		<property name="empid" value="1" />
		<property name="empname" value="Venu" />
		<property name="empsalary" value="21000" />
		<property name="address" ref="address" />
	</bean>
	<bean id="address" class="com.example.demo.model.Address">
		<property name="state" value="Andhrapradesh" />
		<property name="City" value="Guntur" />
		<property name="village" value="Sattenapalli" />
		<property name="Street" value="Nandigama" />
		<property name="pincode" value="522403" />
	</bean>
</beans>